            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="5cda1cb2574fd377ad47423cbfb8d3f787c9f6b5" />
    </application>

</manifest>
